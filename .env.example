# Environment variables for AI Customer Service
# Copy this file to .env and fill in your actual values

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Live Agent Configuration
LIVE_AGENT_NUMBER=+1234567890

# Server Configuration
PORT=3000

# Database Configuration (for PostgreSQL in production)
# DATABASE_URL=postgresql://username:password@localhost:5432/ai_customer_service

